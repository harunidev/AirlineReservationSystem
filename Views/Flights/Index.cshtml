@model IEnumerable<ProjectFinal.Models.Flights>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Plane_model)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cities)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.peoples)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.F_Class)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.F_Image)
        </th>
